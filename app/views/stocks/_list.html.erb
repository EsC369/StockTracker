<table class="table table-striped">
  <thread>
    <tr>
      <th>Name:</th>
      <th>Symbol:</th>
      <th>Current Price:</th>

    <%# Changed from  if @user.id == current_user.id  %>
      <% if @user == current_user %>
      <th>Actions:</th>
      <% end %>

    </tr>
  </thread>
  <tbody>
    <% current_user.stocks.each do |user_stock| %>
    <tr>
      <td><%= user_stock.name %></td>
      <td><%= user_stock.ticker %></td>
      <td><%= user_stock.last_price %></td>
      <% if @user == current_user %>
        <td><%= link_to "Delete This Stock", user_stock_path(user_stock), method: :delete, data: {confirm: "Are You Sure?" }, class: "btn btn-xs btn-danger" %></td>
      <% end %>
    </tr>
    <% end %>
  </tbody>
</table>